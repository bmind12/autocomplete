{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        "@babel/preset-typescript"
    ],
    "plugins": [
        "babel-plugin-styled-components",
        "@babel/proposal-object-rest-spread",
        "@babel/plugin-proposal-class-properties",
        [
            "module-resolver",
            {
                "root": ["./src"],
                "alias": {
                    "components": "./src/components",
                    "ducks": "./src/ducks",
                    "store": "./src/store",
                    "utils": "./src/utils"
                }
            }
        ]
    ]
}
